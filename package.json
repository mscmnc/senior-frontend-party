{
  "name": "react-party",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "compile:sass": "node-sass src/assets/sass/main.scss src/assets/css/main.css -w --output-style expanded ",
    "prefix": "postcss --use autoprefixer -b \"last 10 versions\" src/assets/css/main.css -o src/assets/css/main.prefix.css",
    "compress:css": "node-sass src/assets/css/main.prefix.css src/style.css --output-style compressed"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "node-sass": "^4.10.0",
    "postcss-cli": "^6.0.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
